{
  "name": "jambo-voyage",
  "version": "1.0.0",
  "description": "Nodejs server for Jambo Voyage web application",
  "main": "src/index.js",
  "private": true,
  "scripts": {
    "test": "ava test --verbose --watch",
    "remove-ava-cache": "npx ava --reset-cache",
